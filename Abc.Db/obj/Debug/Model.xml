		<Element Type="SqlProcedure" Name="[dbo].[uspGetProductById]">
			<Property Name="BodyScript">
				<Value><![CDATA[
BEGIN
	SELECT 
		p.[ProductId], 
		p.[Name], 
		p.[Description], 
		p.[ProductTypeId], 
		t.[Name]
	FROM 
		[dbo].[Product] p INNER JOIN [dbo].[ProductType] t ON p.ProductTypeId = t.ProductTypeId
	WHERE
		p.ProductId = @paramProductId
END]]></Value>
			</Property>
